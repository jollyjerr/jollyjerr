{
	"name": "jtabb.dev",
	"version": "0.0.1-alpha",
	"type": "module",
	"private": true,
	"scripts": {
		"setup": "cp .tool-versions.example .tool-versions",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"spellcheck": "cspell src/** knowledge/** posts/** README.md",
		"prepush": "pnpm install && pnpm check && pnpm lint && pnpm spellcheck && pnpm build"
	},
	"devDependencies": {
		"@algolia/client-search": "^5.46.0",
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.1",
		"@notree/server": "^0.2.1",
		"@notree/ui": "^0.3.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"@types/showdown": "^2.0.6",
		"cspell": "9.4.0",
		"date-fns": "^4.1.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"highlight.js": "^11.11.1",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"showdown": "^2.1.0",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"vite": "^7.2.6",
		"wrangler": "^4.53.0"
	},
	"pnpm": {
		"onlyBuildDependencies": [
			"esbuild"
		],
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild",
			"sharp",
			"workerd"
		]
	}
}
